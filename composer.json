{
    "name": "weerd/apollo-pages",
    "description": "Laravel package to provide static page CMS.",
    "keywords": [
        "cms",
        "pages",
        "laravel",
        "weerd"
    ],
    "homepage": "https://github.com/weerd/apollo-pages",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/weerd/apollo-pages/issues",
        "source": "https://github.com/weerd/apollo-pages"
    },
    "authors": [
        {
            "name": "Diego Lorenzo",
            "email": "diego@diegolorenzo.com",
            "homepage": "http://diegolorenzo.com",
            "role": "Developer"
        }
    ],
    "require": {
        "erusev/parsedown": "^1.7",
        "illuminate/console": "~5.5",
        "illuminate/database": "~5.5",
        "illuminate/routing": "~5.5",
        "illuminate/support": "~5.5"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.16",
        "fzaninotto/faker": "^1.7",
        "orchestra/testbench": "~3.0",
        "phpunit/phpunit": "~6.0"
    },
    "autoload": {
        "psr-4": {
            "Weerd\\ApolloPages\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Weerd\\ApolloPages\\Tests\\": "tests"
        }
    },
    "scripts": {
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes",
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
